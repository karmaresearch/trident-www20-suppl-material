PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#> 
SELECT DISTINCT ?var   { ?var ?var4 ?var5 . OPTIONAL {?var5 rdfs:label ?var6} . FILTER(langMatches(lang(?var6),'EN')||(! langMatches(lang(?var6),'*'))) . FILTER(langMatches(lang(?var5),'EN')||(! langMatches(lang(?var5),'*'))) . OPTIONAL {?var4 rdfs:label ?var7}} LIMIT 1000
