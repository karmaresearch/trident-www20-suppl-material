PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#> 
PREFIX foaf: <http://xmlns.com/foaf/0.1/>  
SELECT DISTINCT ?var   { ?var2 a <http://dbpedia.org/ontology/Settlement>; rdfs:label ?var. ?var6 a <http://dbpedia.org/ontology/Airport>. {?var6 <http://dbpedia.org/ontology/city> ?var2} UNION {?var6 <http://dbpedia.org/ontology/location> ?var2} {?var6 <http://dbpedia.org/property/iata> ?var5.} UNION {?var6 <http://dbpedia.org/ontology/iataLocationIdentifier> ?var5. } OPTIONAL { ?var6 foaf:homepage ?var6_home. } OPTIONAL { ?var6 <http://dbpedia.org/property/nativename> ?var6_name.} } LIMIT 1000
